ISO-10303-21;
HEADER;
FILE_DESCRIPTION(('Circuit Breaker - 1 Pole DIN Rail Mount'),'2;1');
FILE_NAME('circuit-breaker.step','2024-01-15T10:00:00',('Engineer'),('PLC Simulator'),'Preprocessor','CAD System','');
FILE_SCHEMA(('AUTOMOTIVE_DESIGN { 1 0 10303 214 1 1 1 1 }'));
ENDSEC;

DATA;
/* Simplified STEP geometry for circuit breaker */
/* In production, this would contain full B-Rep solid geometry */

/* Product Definition */
#1=PRODUCT('Circuit Breaker','1-Pole MCB 10A','DIN Rail Mounted',(#2));
#2=PRODUCT_CONTEXT('',#3,'mechanical');
#3=APPLICATION_CONTEXT('automotive_design');

/* Shape Representation */
#10=SHAPE_REPRESENTATION('Circuit Breaker Shape',(#11),#20);
#11=AXIS2_PLACEMENT_3D('',#12,#13,#14);
#12=CARTESIAN_POINT('',(0.,0.,0.));
#13=DIRECTION('',(0.,0.,1.));
#14=DIRECTION('',(1.,0.,0.));

/* Geometry Context */
#20=GEOMETRIC_REPRESENTATION_CONTEXT(3);
#21=(GEOMETRIC_REPRESENTATION_CONTEXT(3)
GLOBAL_UNCERTAINTY_ASSIGNED_CONTEXT((#22))
GLOBAL_UNIT_ASSIGNED_CONTEXT((#23,#24,#25))
REPRESENTATION_CONTEXT('Context','3D'));
#22=UNCERTAINTY_MEASURE_WITH_UNIT(LENGTH_MEASURE(1.E-06),#23,'distance_accuracy_value','confusion accuracy');
#23=(LENGTH_UNIT()NAMED_UNIT(*)SI_UNIT(.MILLI.,.METRE.));
#24=(NAMED_UNIT(*)PLANE_ANGLE_UNIT()SI_UNIT($,.RADIAN.));
#25=(NAMED_UNIT(*)SI_UNIT($,.STERADIAN.)SOLID_ANGLE_UNIT());

/* Main Body - 18mm x 85mm x 65mm */
#100=BLOCK('Body',(#101),18.,85.,65.);
#101=AXIS2_PLACEMENT_3D('',#102,#103,#104);
#102=CARTESIAN_POINT('',(0.,0.,0.));
#103=DIRECTION('',(0.,0.,1.));
#104=DIRECTION('',(1.,0.,0.));

/* Switch Handle */
#200=BLOCK('Handle',(#201),14.,15.,5.);
#201=AXIS2_PLACEMENT_3D('',#202,#203,#204);
#202=CARTESIAN_POINT('',(2.,80.,35.));
#203=DIRECTION('',(0.,0.,1.));
#204=DIRECTION('',(1.,0.,0.));

/* DIN Rail Clip */
#300=BLOCK('DIN_Clip',(#301),16.,5.,10.);
#301=AXIS2_PLACEMENT_3D('',#302,#303,#304);
#302=CARTESIAN_POINT('',(1.,-5.,0.));
#303=DIRECTION('',(0.,0.,1.));
#304=DIRECTION('',(1.,0.,0.));

ENDSEC;
END-ISO-10303-21;
